		</activity>

		<receiver
			android:name="io.github.gtf.easyShopping.SecretCodeReceiver"
			android:enabled="true"
			android:exported="true">

			<intent-filter>

				<action
					android:name="android.provider.Telephony.SECRET_CODE"/>

