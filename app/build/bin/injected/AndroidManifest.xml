			android:icon="@mipmap/icon"
			android:theme="@style/SplashTheme"
			android:targetActivity=".Main"
			android:enabled="false">

			<intent-filter>

				<action
					android:name="android.intent.action.MAIN"/>

				<category
					android:name="android.intent.category.LAUNCHER"/>

			</intent-filter>

